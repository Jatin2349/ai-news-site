{
  "name": "ai-news-phase0-static",
  "private": true,
  "version": "0.0.2",
  "scripts": {
    "fix:data": "node scripts/fix-data.mjs",
    "validate:data": "node scripts/validate-data.mjs",
    "build": "npm run fix:data && npm run validate:data && next build",
    "dev": "next dev",
    "start": "next start",
    "vercel-build": "npm run fix:data && npm run validate:data && (prisma migrate deploy || prisma db push --accept-data-loss) && prisma generate && next build",
    "deploy": "vercel deploy --prod"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@tailwindcss/typography": "^0.5.15",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.10",
    "autoprefixer": "^10.4.19",
    "lucide-react": "^0.451.0",
    "next": "^16.0.7",
    "openai": "^4.104.0",
    "postcss": "^8.4.41",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^9.0.1",
    "rss-parser": "^3.13.0",
    "swr": "^2.3.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@prisma/internals": "^6.19.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "prisma": "^6.19.0"
  }
}
